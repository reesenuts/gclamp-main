{
  "expo": {
    "name": "gclamp",
    "slug": "gclamp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gc-logo.png",
    "scheme": "gclamp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.zellense.gclamp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/gc-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gc-logo.png",
          "imageWidth": 80,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
